{
  "version": 2,
  "builds": [
    {
      "src": "vercel_app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "vercel_app.py"
    }
  ],
  "functions": {
    "vercel_app.py": {
      "runtime": "python3.9"
    }
  }
}
